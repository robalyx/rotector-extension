{
  "name": "rotector-extension-v2",
  "description": "Real-time warnings about inappropriate Roblox users before you interact with them.",
  "private": true,
  "version": "2.2.2",
  "type": "module",
  "scripts": {
    "dev": "wxt",
    "dev:firefox": "wxt -b firefox",
    "dev:edge": "wxt -b edge",
    "build": "wxt build",
    "build:firefox": "wxt build -b firefox",
    "build:edge": "wxt build -b edge",
    "zip": "wxt zip",
    "zip:firefox": "wxt zip -b firefox",
    "zip:edge": "wxt zip -b edge",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "knip": "knip",
    "knip:production": "knip --production",
    "quality": "bun run check && bun run knip && bun run lint:fix",
    "postinstall": "wxt prepare"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "@wxt-dev/module-svelte": "^2.0.3",
    "autoprefixer": "^10.4.21",
    "eslint": "^9.34.0",
    "eslint-plugin-better-tailwindcss": "^3.7.6",
    "eslint-plugin-svelte": "^3.11.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "globals": "^16.3.0",
    "knip": "^5.63.0",
    "lightningcss": "^1.30.1",
    "postcss": "^8.5.6",
    "svelte": "^5.38.5",
    "svelte-check": "^4.3.1",
    "svelte-eslint-parser": "^1.3.1",
    "tailwindcss": "^4.1.12",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.41.0",
    "wxt": "^0.20.8"
  },
  "dependencies": {
    "svelte-portal": "^2.2.1"
  }
}
